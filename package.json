{
  "name": "project_persional",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "stag": "vue-tsc --noEmit --skipLibCheck && vite build --mode staging",
    "prod": "vue-tsc --noEmit --skipLibCheck && vite build --mode production .npmrc",
    "extract-i18n": "tsnd --transpile-only scripts/extractI18n.ts && prettier --write locales",
    "check:types": "vue-tsc --noEmit --skipLibCheck",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@types/node": "^17.0.25",
    "@vueuse/core": "^7.5.5",
    "axios": "^0.25.0",
    "firebase": "^9.6.11",
    "pinia": "^2.0.13",
    "vue": "^3.2.6",
    "vue-i18n": "^9.2.0-beta.29",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.14.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "object-path": "^0.11.8",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "sass": "^1.50.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.34.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix"
  }
}
